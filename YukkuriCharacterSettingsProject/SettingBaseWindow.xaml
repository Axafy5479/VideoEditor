<Window x:Class="YukkuriCharacterSettingsProject.SettingBaseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YukkuriCharacterSettingsProject"
        xmlns:nav="clr-namespace:YukkuriCharacterSettingsProject.NavigationService"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000"
        nav:NavigationServiceEx.Target="{Binding ElementName=content}"
        nav:NavigationServiceEx.Startup="{x:Type local:YukkuriCharacterSettings}"
        ResizeMode="NoResize"
        WindowStyle="None"
        >
    <Grid>
        <ContentControl x:Name="content" />
    </Grid>
</Window>



